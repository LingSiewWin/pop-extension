{
    "manifest_version": 3,
    "name": "Logo Detection Extension",
    "version": "1.0",
    "description": "Automatically detects logos on web pages using Roboflow's API",
    "permissions": ["activeTab", "tabs", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  